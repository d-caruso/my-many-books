# AWS Cognito Configuration
VITE_COGNITO_USER_POOL_ID=us-west-2_XXXXXXXXX
VITE_COGNITO_USER_POOL_CLIENT_ID=your-client-id-here
VITE_COGNITO_IDENTITY_POOL_ID=us-west-2:xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
VITE_AWS_REGION=us-west-2


# API Configuration
VITE_API_BASE_URL=https://your-api-id.execute-api.us-west-2.amazonaws.com/dev/api/v1

# Language Selector
VITE_SHOW_LANGUAGE_SELECTOR=true

# Default Pagination
VITE_BOOKS_PAGINATION_DEFAULT=5

# Development settings
REACT_APP_ENV=development

# PWA settings
GENERATE_SOURCEMAP=false
REACT_APP_PWA_ENABLED=true

# Service Worker
PUBLIC_URL=/

# Scanner settings
REACT_APP_ENABLE_CAMERA_SCANNER=true
REACT_APP_SCANNER_DEBUG=false