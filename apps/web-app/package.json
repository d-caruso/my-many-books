  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "craco test --coverage --watchAll=false",
    "test:unit": "craco test --testPathPattern=src/ --watchAll=false",
    "test:unit:watch": "craco test --testPathPattern=src/",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:component": "cypress run --component",
    "cypress:component:open": "cypress open --component",
    "e2e": "start-server-and-test start http://localhost:3000 cypress:run",
    "e2e:open": "start-server-and-test start http://localhost:3000 cypress:open",
    "test:ci": "craco test --coverage --ci --watchAll=false --testResultsProcessor=jest-sonar-reporter",
    "test:all": "npm run test:unit && npm run cypress:component && npm run e2e",
    "test:coverage:full": "npm run test:coverage && npm run cypress:run --coverage",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
